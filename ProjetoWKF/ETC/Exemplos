Mapa:

// Cenario hipotetico

Necessidade: Recebe valores do CSV, encontrar chaves com determinado caractere, formatar as mesmas,
achar os valores referentes a essas chaves

List newListWithCustomFieldsFormatted = []

List newListWithCustomFieldsValues = []

Map csvFields = [
        "(CF) Código de rede": "valor Código",
        "serviceOrderCode"   : "valor Código da OS",
        "(CF) XPTO"          : "valor XPTO",
        "(CF) NF compra"     : "valor NF",
        "serviceOrderCode"   : "valor serviceOrderCode",
        "notes"              : "Bem observado",
        "informedDefectCode" : "valor informedDefectCode"
]

def csvCustomFieldNameAsList = csvFields.keySet().findAll {
    it.startsWith("(CF) ")
} as List

csvCustomFieldNameAsList.each {
    newListWithCustomFieldsFormatted << it.replace("(CF) ", "")
}

println "Chaves que contem (CF) no nome: [${csvCustomFieldNameAsList}]"
println "\n"

csvFields.each {
    if (it.key.startsWith('(CF) ')) {
        newListWithCustomFieldsValues << it.value
    }
}

println "Valores das chaves que contem (CF) no nome: [${newListWithCustomFieldsValues}]"
